.form-horizontal
  .control-group
    = f.label :address, "Your  Address", class:"control-label" 
    .controls
      = f.text_field :address  
  .control-group
    = f.label :phone_number, "Your Phone Number", class:"control-label" 
    .controls
      = f.text_field :phone_number 
  .control-group              
    = f.label :data_of_birth, "Your Data of Birth", class:"control-label" 
    .controls
      =# f.calendar_date_select :data_of_birth 
      = f.text_field :data_of_birth, :id => 'birthDate', :as => :ui_date_time_picker
      =# f.datetime_field :data_of_birth, datetimepicker: { showMinute: false }
      




:javascript
  $(document).ready(function() {
    $("#birthDate").datepicker({dateFormat: 'yy-mm-dd'});
    )};